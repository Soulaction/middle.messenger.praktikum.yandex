.page-profile-content {
    align-items: center;
    display: flex;
    flex-direction: column;
    height: calc(100vh - 60px);
    justify-content: flex-start;
    margin-top: 60px;
    width: 100vw;
}

.user-name {
    font-size: 16px;
    line-height: 16px;
    margin-top: 21px;
}

.user-info-border {
    border: 1px solid #eaeaea;
    margin: 10px 0;
}

.user-info-footer {
    align-items: flex-start;
    display: flex;
    flex-direction: column;
    margin-top: 60px;
}
